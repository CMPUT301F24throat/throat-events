<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:right="8dp" android:left="8dp" android:top="8dp" android:bottom="8dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#E0E0E0"/> <!-- Light gray background -->
            <corners android:radius="50dp"/>  <!-- Circular shape -->
        </shape>
    </item>
    <item android:left="0dp" android:right="0dp" android:top="0dp" android:bottom="0dp">
        <bitmap
            android:src="@drawable/ic_profile_name"
        android:gravity="center"/>
    </item>
</layer-list>
